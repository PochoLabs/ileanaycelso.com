			link(rel='stylesheet', href='assets/css/ie8.css')
	body
		// Wrapper
		#wrapper
			// Header
			header#header.alt
				a.logo(href='#page-top')
					strong Ileana
					span &
					strong Celso
				nav
					a(href='#menu') Menu
			// Menu
			nav#menu
				ul.links
					li
						a(href='#page-top') Home
					li
						a(href='#about') Who We Are 
					li
						a(href='#media') Generic

				ul.actions.vertical
					li
						a.button.special.fit(href='#') Contact Us 
			// Banner
			section#banner.major
				.inner
					header.major
						h1 Singing songs of inspiration and social justice
					.content
						p
							| DREAMers brought together by music and
							br
							| 								the migrant justice movement.
						ul.actions
							li
								a.button.next.scrolly(href='#one') Get Started
			// Main
			#main
				// One
				section#one.tiles
					article
						span.image
							img(src='images/pic01.jpg', alt='')
						header.major
							h3
								a.link(href='landing.html') Aliquam
							p Ipsum dolor sit amet
					article
						span.image
							img(src='images/pic02.jpg', alt='')
						header.major
							h3
								a.link(href='landing.html') Tempus
							p feugiat amet tempus
					article
						span.image
							img(src='images/pic03.jpg', alt='')
						header.major
							h3
								a.link(href='landing.html') Magna
							p Lorem etiam nullam
					article
						span.image
							img(src='images/pic04.jpg', alt='')
						header.major
							h3
								a.link(href='landing.html') Ipsum
							p Nisl sed aliquam
					article
						span.image
							img(src='images/pic05.jpg', alt='')
						header.major
							h3
								a.link(href='landing.html') Consequat
							p Ipsum dolor sit amet
