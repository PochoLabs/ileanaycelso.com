						ul.actions
							li
								a.button.next(href='landing.html') Get Started
			// Contact
			section#contact
				.inner
					section
						form(method='post', action='#')
							.field.half.first
								label(for='name') Name
								input#name(type='text', name='name')
							.field.half
								label(for='email') Email
								input#email(type='text', name='email')
							.field
								label(for='message') Message
								textarea#message(name='message', rows='6')
							ul.actions
								li
									input.special(type='submit', value='Send Message')
								li
									input(type='reset', value='Clear')
					section.split
						section
							.contact-method
								span.icon.alt.fa-envelope
								h3 Email
								a(href='#') information@untitled.tld
						section
							.contact-method
								span.icon.alt.fa-phone
								h3 Phone
								span (000) 000-0000 x12387
						section
							.contact-method
								span.icon.alt.fa-home
								h3 Address
								span
									| 1234 Somewhere Road #5432
									br
									| 										Nashville, TN 00000
									br
									| 										United States of America
			// Footer
			footer#footer
				.inner
					ul.icons
						li
							a.icon.alt.fa-twitter(href='#')
								span.label Twitter
						li
							a.icon.alt.fa-facebook(href='#')
								span.label Facebook
